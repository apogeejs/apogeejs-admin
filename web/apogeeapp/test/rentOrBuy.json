					"name": "calculateSavings",
					"type": "apogee.FolderFunction",
					"argList": [
						"expenses"
					],
					"returnValue": "savings",
					"internalFolder": {
						"name": "calculateSavings",
						"type": "apogee.Folder",
						"children": {
							"expenses": {
								"name": "expenses",
								"type": "apogee.JsonTable",
								"updateData": {
									"argList": [],
									"functionBody": "var value;\nvalue = [];\nfor(var i = 0; i < settings.generalInfo.numberMonths+1; i++) {\n    value[i] = 1000;\n}\nreturn value;\n\n",
									"supplementalCode": "",
									"description": ""
