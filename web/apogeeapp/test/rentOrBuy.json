						"taxCreditArray": {
							"name": "taxCreditArray",
							"type": "apogee.JsonTable",
							"updateData": {
								"argList": [],
								"functionBody": "var value;\nvar marginalTaxFraction = settings.ownerInfo.marginalTaxRate / 100; \nvalue = utils.arrayMultiply(loanArrays.interestArray,marginalTaxFraction);\nreturn value;\n\n",
								"supplementalCode": "",
								"description": ""
