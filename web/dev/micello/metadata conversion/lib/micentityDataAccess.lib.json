				"getRecordInfo": {
					"name": "getRecordInfo",
					"type": "apogee.FunctionTable",
					"updateData": {
						"argList": [
							"recordType"
						],
						"functionBody": "var recordInfo = dataSetInfo.records[recordType];\n\nif(!recordInfo) {\n    throw new Error(\"Record type not found in getRecordInfo: \" + recordType);\n}\n\nreturn recordInfo;",
						"supplementalCode": "",
